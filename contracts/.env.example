# Environment variables for testnet deployment
# Copy this file to .env and fill in your values

# Testnet RPC URLs (get from Infura/Alchemy/QuickNode)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_PROJECT_ID
MAINNET_RPC_URL=https://mainnet.infura.io/v3/YOUR_PROJECT_ID

# Your MetaMask private key (TESTNET ACCOUNT ONLY!)
# Never use your mainnet private key for testing!
PRIVATE_KEY=your_metamask_private_key_here

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Example values (replace with your own):
# SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/9aa3d95b3bc440fa88ea12eaa4456161
# PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef